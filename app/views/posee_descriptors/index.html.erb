<h1>Listado "Posee Descriptores"</h1>

<table>
  <tr>
    <th>Norma</th>
    <th>Descriptor general</th>
  </tr>

<% for posee_descriptor in @posee_descriptors %>
  <tr>
    <td><%=h posee_descriptor.norma.numero %></td>
    <td><%=h posee_descriptor.descriptor_general.nombre %></td>
    <td><%= link_to 'Mostrar', posee_descriptor %></td>
    <td><%= link_to 'Editar', edit_posee_descriptor_path(posee_descriptor) %></td>
    <td><%= link_to 'Eliminar', posee_descriptor, :confirm => '¿Está Seguro?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nuevo "Posee Descriptor"', new_posee_descriptor_path %> |
<%= link_to 'Atrás', :back %> |
<%= link_to 'Principal', :controller => 'pagina_principal' %>