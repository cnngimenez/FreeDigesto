<h1>Editar "Posee Descriptor Particular"</h1>

<% if false %>
<% form_for(@posee_desc_part) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :norma %><br />
    <%= f.text_field :norma %>
  </p>
  <p>
    <%= f.label :descriptor_particular %><br />
    <%= f.text_field :descriptor_particular %>
  </p>
  <p>
    <%= f.submit "Cambiar" %>
  </p>
<% end %>

<%= link_to 'Show', @posee_desc_part %> |
<%= link_to 'Back', posee_desc_parts_path %>
<% end %>

<%= link_to 'Atrás', :back %> |
<%= link_to 'Ir al listado', :action => 'index' %> |
<%= link_to 'Principal', :controller => 'pagina_principal' %>