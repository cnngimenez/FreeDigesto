<p id="notice"><%= notice %></p>

<h1>Tipo Relacions</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Comentario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_relacions.each do |tipo_relacion| %>
      <tr>
        <td><%= tipo_relacion.nombre %></td>
        <td><%= tipo_relacion.comentario %></td>
        <td><%= link_to 'Show', tipo_relacion %></td>
        <td><%= link_to 'Edit', edit_tipo_relacion_path(tipo_relacion) %></td>
        <td><%= link_to 'Destroy', tipo_relacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Relacion', new_tipo_relacion_path %>
