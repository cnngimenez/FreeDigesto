<p id="notice"><%= notice %></p>

<h1>Tipo Normas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Comentario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_normas.each do |tipo_norma| %>
      <tr>
        <td><%= tipo_norma.nombre %></td>
        <td><%= tipo_norma.comentario %></td>
        <td><%= link_to 'Show', tipo_norma %></td>
        <td><%= link_to 'Edit', edit_tipo_norma_path(tipo_norma) %></td>
        <td><%= link_to 'Destroy', tipo_norma, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Norma', new_tipo_norma_path %>
